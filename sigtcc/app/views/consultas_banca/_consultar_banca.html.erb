
<% if !bancas.nil? %>
<table id= "bancas">
  <thead>
    <tr>
      <th>sala</th>
      <th>horario</th>
      <!-- <th>titulo</th>
      <th>Orientador</th> -->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

    <% bancas.each do |banca| %>
      <tr>
        <<td > <%= banca.sala %></td>
        <td ><%= banca.horario %></td>
      <!--  <td ><%= banca.tcc.titulo %></td> -->
        <td ><%=link_to "Expandir", banca  %></td>
      </tr>
    <%end%>
  </tbody>
</table>
<% end %>
